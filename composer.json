{
    "name": "railken/cli-amethyst",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "doctrine/inflector": "^1.3",
        "eloquent/composer-config-reader": "^2.1",
        "railken/bag": "^1.2",
        "corneltek/getoptionkit": "^2.6",
        "symfony/console": "^4.2",
        "laravel/installer": "^2.0",
        "rcrowe/twigbridge": "^0.9.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "danielstjules/stringy": "~3.1.0",
        "railken/amethyst-common": "0.1.*",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.11.0"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\Cli\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Cli\\Tests\\": "tests/",
            "Author\\Test\\": "var/cache/src"
        }
    },
    "bin": [
        "bin/amethyst"
    ]
}
