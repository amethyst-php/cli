{
    "name": "author/amethyst-package-name",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-package-name", "package-name"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*"
    },
    "autoload": {
        "psr-4" : {
            "MyEscapedNamespace\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "MyEscapedNamespace\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "package-name",
            "namespace": "MyEscapedNamespace"
        },
        "laravel": {
            "providers": [
                "MyEscapedNamespace\\Providers\\PackageNameServiceProvider"
            ]
        }
    }
}
